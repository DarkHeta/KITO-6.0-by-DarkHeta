material gloweye1Mat
{
	technique
	{
		pass
		{
			emissive 1 1 1 1
			texture_unit
			{
				texture  red.jpg
			}
			texture_unit
			{
				texture  glowTex.jpg
				scale 40 40
				scroll_anim 0.2 0
				colour_op add
			}
		}
	}
}
