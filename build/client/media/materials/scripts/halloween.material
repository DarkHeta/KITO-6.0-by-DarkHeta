material candleFlameMat
{
	technique
	{
		pass
		{
			lighting off
			scene_blend add
			depth_write off
			cull_hardware none
			cull_software none
			texture_unit
			{
				texture  candleFlameTex.jpg
				wave_xform scale_x sine 0.6 6 0 0.2
				wave_xform scale_y sine 0.6 6 0.35 0.3
				tex_address_mode clamp
			}
		}
	}
}