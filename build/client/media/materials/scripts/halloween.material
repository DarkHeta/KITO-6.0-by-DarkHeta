material pumpkinMat
{
	technique
	{
		pass
		{
			texture_unit
			{
				texture  pumpkinTex.jpg
			}
		}
	}
}
material pumpkinGlowMat
{
	technique
	{
		pass
		{
			emissive 1 1 1 1
			texture_unit
			{
				texture  pumpkinTex.jpg
				wave_xform scale_x sine 1.7 1 0 1.7
				wave_xform scale_y sine 1.7 1 0 1.7
			}
		}
	}
}
