material bubbleMat
{
	technique
	{
		pass
		{
			lighting off
			depth_write off
			scene_blend add
			cull_hardware none
			cull_software none
			texture_unit
			{
				texture bubbleTex.jpg
				scroll_anim -0.1 0
			}
		}
	}
}
