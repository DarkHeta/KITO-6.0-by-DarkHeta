	{
		pass
		{
			lighting off
			scene_blend alpha_blend
			depth_check off

			texture_unit
			{
				texture ogreborderUp.png
			}
		}
	}
}
material Sky/CloudySky
{
	technique
	{
		pass
		{
			lighting off
			depth_write off

			texture_unit
			{
				texture clouds.jpg
				scroll_anim 0.15 0
			}
		}
	}
}
material Sky/Clouds
{
	technique
	{
		pass
		{
			lighting off
			depth_write off
			scene_blend alpha_blend
			fog_override true

			texture_unit
			{
				texture clouds.PNG
				scroll_anim 0.15 0
			}
		}
	}
}
material Sky/Shader
{
	technique
	{
		pass
		{
			depth_write off
			scene_blend add
			fog_override true
			ambient 0 0 0
			diffuse 0 0 0 0
			emissive 0 0 0 0.0

			texture_unit
			{
				texture shader.jpg
			}
		}
	}
}
material Sky/Fader
{
	technique
	{
		pass
		{
			depth_write off
			scene_blend modulate
			fog_override true
			ambient 0 0 0
			diffuse 0 0 0 0
			emissive 1 1 1 0.0

			texture_unit
			{
				texture shader.jpg
			}
		}
	}
}
