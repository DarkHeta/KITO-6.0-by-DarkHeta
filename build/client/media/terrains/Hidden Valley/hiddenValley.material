material Terrain/HiddenValleyTerrain
{
	receive_shadows on
	technique
	{
		pass
		{
			texture_unit
			{
				texture grass_tex2.png
				scale 0.025 0.025
			}
		}
		pass
		{
			ambient 1 1 1 1
			scene_blend zero src_colour
		} 	
	}
}
material Waterfall2
{
	technique
	{
		pass
		{
			cull_hardware none
			cull_software none
			scene_blend alpha_blend
			depth_write off

			texture_unit
			{
				texture waterfall2.png
				scroll_anim 0 0.75
			}
			texture_unit
			{
				texture waterfallmask.png
				colour_op alpha_blend
				tex_address_mode clamp
			}
		}
	}
}
material GateMatValley
{
	receive_shadows off
	technique
	{
		pass
		{
			scene_blend add

			texture_unit
			{
				texture portalHiddenValley.jpg
				env_map spherical
			}
		}
		pass
		{
			scene_blend modulate

			texture_unit
			{
				texture portalHiddenValley.jpg
				env_map spherical
			}
		}
	}
}