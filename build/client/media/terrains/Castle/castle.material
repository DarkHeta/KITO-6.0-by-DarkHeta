material Terrain/castleterrain
{
    receive_shadows on
    technique
    {
        pass
        {
            texture_unit
            {
                texture crystal_tex2.jpg
                scale 0.05 0.05
 scroll_anim -0.01 -0.01
            }
        }
	pass
        {
            scene_blend alpha_blend
            depth_func equal
            texture_unit
            {
                texture castleterrainmask.png
                alpha_op_ex source1 src_texture src_texture
                colour_op_ex source2 src_texture src_texture
            }
            texture_unit
            {
              	texture grass_tex2.png
		scale 0.05 0.05
                colour_op_ex blend_diffuse_alpha src_texture src_current
            }
        }
        pass
        {
            scene_blend alpha_blend
            depth_func equal
            texture_unit
            {
                texture castleterrainmask2.png
                alpha_op_ex source1 src_texture src_texture
                colour_op_ex source2 src_texture src_texture
            }
            texture_unit
            {
              	texture rock_tex1.png
		scale 0.05 0.05
                colour_op_ex blend_diffuse_alpha src_texture src_current
            }
        }
		pass
		{
			ambient 1 1 1 1
			scene_blend zero src_colour
		} 	
        
    }
}
material Portal/Castle
{
	receive_shadows off
	technique
	{
		pass
		{
			lighting off
			scene_blend add

			texture_unit
			{
				texture portalCastle.jpg
				env_map spherical
				rotate_anim 0.1
			}
		}
		pass
		{
			lighting off
			scene_blend modulate

			texture_unit
			{
				texture portalCastle.jpg
				env_map spherical
				rotate_anim 0.1
			}
		}
	}
}
